{% extends 'DeepMikotoAdminBundle::layout.html.twig' %}
{% block divided_layout %}
    <div class="container">
        <div class="col-lg-2 col-md-2 col-sm-4 col-xs-12 pull-left sidebar-block">
            {% block sidebar %}
            {% endblock %}
        </div>
        <div class="col-lg-10 col-md-10 col-sm-7 col-xs-12 pull-left content-block">
            <div class="panel">
                <div class="panel-body">
                    {% block content %}
                    {% endblock %}
                </div>
            </div>
        </div>
    </div>
{% endblock %}
