{% include 'DeepMikotoApiBundle:miscellaneous:source_signature.html.twig' %}
{% spaceless %}
    <!DOCTYPE html>
    <html lang="en">
    <head>
        {% block meta %}{% endblock %}
        {% image '@DeepMikotoApiBundle/Resources/public/images/deepmikoto_logo_16.png' output='images/favicon.png' %}
            <link rel="icon" type="image/png" href="{{ asset_url }}">
        {% endimage %}
        {% stylesheets
            'bundles/deepmikotoapi/css/bootstrap.css'

            output='css/coding-demo.css' filter='cssrewrite'
        %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
        {% endstylesheets %}
        <link href='https://fonts.googleapis.com/css?family=Roboto+Condensed:400,700' rel='stylesheet' type='text/css'>

        <!-- demo styles -->

        {% block style %}{% endblock %}

        <!-- end demo styles -->

    </head>
    <body>
    <div class="col-xs-12">
        <div class="row">
            <div class="col-xs-12">
                {% block postInfo %}{% endblock %}
            </div>
        </div>
        <hr>
        <div class="row">
            <div class="col-xs-12">
                <!-- demo html -->

                {% block html %}{% endblock %}

                <!-- end demo html -->
            </div>
        </div>
    </div>

    <!-- demo js -->

    {% block js %}{% endblock %}

    <!-- end demo js -->

    <!-- Google Analytics -->

    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                    (i[r].q=i[r].q||[]).push(arguments)};i[r].l=new Date();a=s.createElement(o);
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
        /** @namespace ga */
        ga('create', 'UA-56243816-2', 'auto');
    </script>
    </body>
    </html>
{% endspaceless %}
